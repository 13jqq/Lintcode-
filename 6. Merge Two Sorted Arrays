public class Solution {
    /*
     * @param A: sorted integer array A
     * @param B: sorted integer array B
     * @return: A new sorted integer array
     */
    public int[] mergeSortedArray(int[] A, int[] B) {
        // write your code here
        int len1 = A.length;
        int len2 = B.length;
        int[] C = new int[len1+len2];
        int i = 0, j = 0 , index = 0 ;
        if(len1 ==  0 ||  len2==0 ){
        	return (A == null ? B : A);
        }
        while( i <len1 && j< len2 ){
        	if(A[i] <=  B[j]){
        		C[index++] = A[i];
        		i++;
        	}
        	else{
        		C[index++] = B[j++];
        	}
        }
        while(i < len1){
        	C[index++] = A[i++];
        }
        while(j <len2){
        	C[index ++ ] = B[j++];
        }
        return C;
        
    }
    
}
