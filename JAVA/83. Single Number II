public class Solution {
    /**
     * @param A: An integer array
     * @return: An integer
     */
   public int singleNumberII(int[] A) {
	        // write your code here
		 
		 if(  A.length == 1){
			 return A[0];
		 }
		 int count = 0 ;
		 int single  = 0;
		 int index = 0 ;
		 for( int  i = 0 ; i < A.length ; i++){
			 index = A[i];
			 for( int j = 0 ; j < A.length ; j++){
				 if( A[j] == index){
					 count ++;
				 }
			 }
			 if( count == 1){
				 single = index;
				 continue;
			 }
			 if( count == 3){
				 count = 0 ;
			 }
			 count = 0;
			 
		 }
		 return single;
	    }
}
